<application>
	<name>Inertial_sensors_auto_calibration-torso</name>
	<dependencies>
		<port>/icub/torso/inertialMTB</port>
		<port>/icub/torso/stateExt:o</port>
	</dependencies>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/torso/inertialMTB --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-torso-inertialMTB</tag>
	</module>
	<connection>
		<from>/icub/torso/inertialMTB</from>
		<to>/dumper/icub/torso/inertialMTB</to>
		<protocol>udp</protocol>
	</connection>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/torso/stateExt:o --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-torso-stateExt:o</tag>
	</module>
	<connection>
		<from>/icub/torso/stateExt:o</from>
		<to>/dumper/icub/torso/stateExt:o</to>
		<protocol>udp</protocol>
	</connection>
	<module>
		<name>reachRandomJointPositions</name>
		<parameters>--context AccelAutoCalib --from randomJointsLeftLeg.ini</parameters>
		<node>localhost</node>
		<tag>randomJoints-torso</tag>
	</module>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/torso/return --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-torso-return</tag>
	</module>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/torso/fitting --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-torso-fitting</tag>
	</module>
	<connection>
		<from>/reachGridJointPositions/icub/torso/isTheRobotInReturnPoint:o</from>
		<to>/dumper/icub/torso/return</to>
		<protocol>tcp</protocol>
	</connection>
	<connection>
		<from>/reachGridJointPositions/icub/torso/useFurtherPosForFitting:o</from>
		<to>/dumper/icub/torso/fitting</to>
		<protocol>tcp</protocol>
	</connection>
</application>
