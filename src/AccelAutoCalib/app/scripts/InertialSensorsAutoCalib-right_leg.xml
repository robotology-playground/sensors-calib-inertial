<application>
	<name>Inertial_sensors_auto_calibration-right_leg</name>
	<dependencies>
		<port>/icub/right_leg/inertialMTB</port>
		<port>/icub/right_leg/stateExt:o</port>
	</dependencies>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/right_leg/inertialMTB --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-right_leg-inertialMTB</tag>
	</module>
	<connection>
		<from>/icub/right_leg/inertialMTB</from>
		<to>/dumper/icub/right_leg/inertialMTB</to>
		<protocol>udp</protocol>
	</connection>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/right_leg/stateExt:o --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-right_leg-stateExt:o</tag>
	</module>
	<connection>
		<from>/icub/right_leg/stateExt:o</from>
		<to>/dumper/icub/right_leg/stateExt:o</to>
		<protocol>udp</protocol>
	</connection>
	<module>
		<name>reachRandomJointPositions</name>
		<parameters>--context AccelAutoCalib --from randomJointsLeftLeg.ini</parameters>
		<node>localhost</node>
		<tag>randomJoints-right_leg</tag>
	</module>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/right_leg/return --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-right_leg-return</tag>
	</module>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/right_leg/fitting --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-right_leg-fitting</tag>
	</module>
	<connection>
		<from>/reachGridJointPositions/icub/right_leg/isTheRobotInReturnPoint:o</from>
		<to>/dumper/icub/right_leg/return</to>
		<protocol>tcp</protocol>
	</connection>
	<connection>
		<from>/reachGridJointPositions/icub/right_leg/useFurtherPosForFitting:o</from>
		<to>/dumper/icub/right_leg/fitting</to>
		<protocol>tcp</protocol>
	</connection>
</application>
