<application>
	<name>Inertial_sensors_auto_calibration-left_arm</name>
	<dependencies>
		<port>/icub/left_hand/inertialMTB</port>
		<port>/icub/left_arm/stateExt:o</port>
	</dependencies>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/left_arm/inertialMTB --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-left_arm-inertialMTB</tag>
	</module>
	<connection>
		<from>/icub/left_hand/inertialMTB</from>
		<to>/dumper/icub/left_arm/inertialMTB</to>
		<protocol>udp</protocol>
	</connection>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/left_arm/stateExt:o --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-left_arm-stateExt:o</tag>
	</module>
	<connection>
		<from>/icub/left_arm/stateExt:o</from>
		<to>/dumper/icub/left_arm/stateExt:o</to>
		<protocol>udp</protocol>
	</connection>
	<module>
		<name>reachRandomJointPositions</name>
		<parameters>--context AccelAutoCalib --from randomJointsLeftArm.ini</parameters>
		<node>localhost</node>
		<tag>randomJoints-left_arm</tag>
	</module>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/left_arm/return --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-left_arm-return</tag>
	</module>
	<module>
		<name>yarpdatadumper</name>
		<parameters>--name /dumper/icub/left_arm/fitting --type bottle </parameters>
		<node>localhost</node>
		<tag>data-dumper-left_arm-fitting</tag>
	</module>
	<connection>
		<from>/reachGridJointPositions/left_arm/isTheRobotInReturnPoint:o</from>
		<to>/dumper/icub/left_arm/return</to>
		<protocol>tcp</protocol>
	</connection>
	<connection>
		<from>/reachGridJointPositions/left_arm/useFurtherPosForFitting:o</from>
		<to>/dumper/icub/left_arm/fitting</to>
		<protocol>tcp</protocol>
	</connection>
</application>
